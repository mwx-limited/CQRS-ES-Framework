{
  "name": "mwx-limited/cqrs-es-framework",
  "description": "Smooth Php CRQS Event Sourcing Framework",
  "keywords": [
    "smooth php",
    "Command Bus",
    "Event Sourcing",
    "CRQS"
  ],
  "homepage": "https://github.com/mwx-limited/CQRS-ES-Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Bennett",
      "email": "simon@bennett.im",
      "homepage": "https://smoothphp.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ramsey/uuid": "~3.0",
    "nesbot/carbon": "~1.19"
  },
  "replace": {
    "smoothphp/commandbus": "self.version",
    "smoothphp/contracts": "self.version",
    "smoothphp/domain": "self.version",
    "smoothphp/eventbus": "self.version",
    "smoothphp/eventdispatcher": "self.version",
    "smoothphp/eventsourcing": "self.version",
    "smoothphp/serialization": "self.version",
    "smoothphp/eventstore": "self.version"

  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "SmoothPhp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmoothPhp\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
